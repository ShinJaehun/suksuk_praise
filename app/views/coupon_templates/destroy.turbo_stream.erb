<%= turbo_stream.update "mine" do %>
  <%= render partial: "mine_section", locals: { items: @mine_rows } %>
<% end %>
<%# 관리자이고 방금 라이브러리를 삭제했다면 라이브러리도 새로 그림 %>
<% if defined?(@library) && @library.present? %>
  <%= turbo_stream.update "library" do %>
    <%= render partial: "library_section", locals: { templates: @library, library_admin: true } %>
  <% end %>
<% end %>

<%= turbo_stream.update "modal", "" %>
